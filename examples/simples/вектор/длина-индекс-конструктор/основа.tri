модуль длина-индекс-конструктор

импорт "/home/cyrus/trivil/examples/simples/вывод"


тип Числа = []Цел64

вход{
    пусть кол-во = 5
    пусть числа = Числа[длина: кол-во, *:-10, 1: 5, 3: 4]

    пусть № := 0

    пока № < кол-во {
        вывод.цел64(числа[№])
        вывод.строка(" ")
        № := № + 1
    }
    вывод.кс()
}
