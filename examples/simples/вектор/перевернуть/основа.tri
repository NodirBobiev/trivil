импорт "/home/cyrus/trivil/examples/simples/вывод"
импорт "/home/cyrus/trivil/examples/simples/ввод"

тип Числа = []Цел64

вход{
    пусть Н :Цел64 := 0
    Н := ввод.цел64()

    пусть числа = Числа[длина: Н, *:0]

    пусть № := 0

    пока № < Н {
        числа[№] := ввод.цел64()
        № := № + 1
    }

    № := Н - 1

    пока № >= 0 {
        вывод.цел64(числа[№])
        вывод.строка(" ")
        № := № - 1
    }

    вывод.кс()
}
