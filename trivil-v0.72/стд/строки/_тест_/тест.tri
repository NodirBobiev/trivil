импорт "стд::тестирование/тест-основа"

фн Тестировать*(т: тест-основа.Основа) {
    т.тест("префикс 1", есть префикс("привет", "при"))
    т.тест("префикс 2", ~ есть префикс("привет", "пре"))

    т.тест("суффикс 1", есть суффикс("привет", "вет"))
    т.тест("суффикс 2", ~ есть суффикс("привет", "ват"))

    т-соединить(т)
    т-строка в цел(т)
}

